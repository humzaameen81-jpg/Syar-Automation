{
  "action": {
    "default_icon": "icon-128.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.bundle.js",
    "type": "module"
  },
  "commands": {
    "element-picker": {
      "description": "Open element picker",
      "suggested_key": {
        "default": "Alt+P",
        "mac": "Alt+P"
      }
    },
    "open-dashboard": {
      "description": "Open the dashboard",
      "suggested_key": {
        "default": "Alt+A",
        "mac": "Alt+A"
      }
    }
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": [
        "contentScript.bundle.js"
      ],
      "match_about_blank": true,
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    },
    {
      "all_frames": false,
      "js": [
        "webService.bundle.js"
      ],
      "matches": [
        "http://localhost/*",
        "*://*.automa.site/*",
        "*://automa.vercel.app/*"
      ],
      "run_at": "document_start"
    }
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "key": "",
  "manifest_version": 3,
  "minimum_chrome_version": "116",
  "name": "Syar Automation",
  "optional_permissions": [
    "cookies",
    "downloads",
    "contextMenus",
    "clipboardRead",
    "notifications"
  ],
  "permissions": [
    "tabs",
    "proxy",
    "alarms",
    "storage",
    "debugger",
    "activeTab",
    "offscreen",
    "webNavigation",
    "unlimitedStorage",
    "scripting"
  ],
  "sandbox": {
    "pages": [
      "/sandbox.html"
    ]
  },
  "version": "1.29.12",
  "web_accessible_resources": [
    {
      "matches": [
        "*://*/*"
      ],
      "resources": [
        "/locales/*",
        "/icon-128.png",
        "/elementSelector.css",
        "elementSelector.bundle.js",
        "/Inter-roman-latin.var.woff2"
      ]
    }
  ]
}